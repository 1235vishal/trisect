.top {
    background-color: #FFFFFF;
    width: 100%;
    z-index: 999;
    display: flex;
    flex-direction: column;
    /* Allow stacking for responsive behavior */

    .nav {
        height: 12vh;
        width: 100%;
        background-color: #ffffff;
        display: flex;
        align-items: center;
        position: relative;
        position: static;
        justify-content: space-between;
        padding-left: 2%;
        padding-right: 2%;
        /* Add right padding for spacing */
    }

    .logo img {
        max-width: 150px;
        height: auto;
        margin-left: 10%;
    }

    .options {
        display: flex;
        gap: 4vh;
        transition: transform 0.3s ease;
        margin-left: -25%;
        /* Removed negative margin */
    }
    .options a {
        color: #060606;
        text-decoration: none;
        font-size: 1.6rem;
        /* Font size remains the same */
        font-weight: 600;
        position: relative;
        /* Needed for positioning the pseudo-element */
        overflow: hidden;
        /* Ensures the animation stays within the link */
    }

    .options a::after {
        content: '';
        position: absolute;
        bottom: 0;
        left: 0;
        /* Positioned at the start (left) of the link */
        width: 100%;
        /* Full width when animated */
        height: 3px;
        background-color: #451591;
        transform: scaleX(0);
        /* Initially hidden */
        transform-origin: left;
        /* The border grows from the left */
        transition: transform 0.4s ease;
        /* Smooth transition for the scale effect */
    }

    .options a:hover::after {
        transform: scaleX(1);
        /* Expands the border from left to right */
    }

    .options a:hover {
        color: #353535;
        transition: color 0.3s ease;
    }

    .options a:not(:hover)::after {
        transform: scaleX(0);
        /* Hides the border by shrinking it from the left */
        transform-origin: right;
        /* Shrinks back from right to left */
    }




    .toggle {
        display: none;
        font-size: 5.4rem;
        /* Was 24px, now 2.4rem */
        cursor: pointer;
    }

    .rightbtn {
        background: linear-gradient(#ff68eb, #822291, #581991);
        /* Gradient background */
        color: #fff;
        /* Default text color */
        height: 100%;
        /* Full height */
        margin-right: -2.5%;
        /* Right margin */
        display: flex;
        /* Flexbox for centering content */
        align-items: center;
        /* Vertical centering */
        justify-content: center;
        /* Horizontal centering */
        font-size: 1.6rem;
        /* Font size */
        font-weight: 500;
        /* Font weight */
        width: 12vw;
        /* Width */
        cursor: pointer;
        /* Pointer cursor on hover */
        position: relative;
        /* Position for the pseudo-element */
        overflow: hidden;
        /* Hide overflow for the after pseudo-element */
        transition: color 0.3s ease, transform 0.3s ease;
        /* Smooth transition for text color and transform */
    }

    /* Hover Effect */
    .rightbtn:hover {
        color: #000;
        /* Change text color to black on hover */
        transform: scale(1.01);
        /* Slightly enlarge the button on hover */
    }

    /* Button After Pseudo-element for Background */
    .rightbtn:after {
        content: '';
        /* Required for pseudo-element */
        position: absolute;
        /* Positioning */
        top: 0;
        /* Align to the top */
        left: 0;
        /* Align to the left */
        width: 0;
        /* Initial width */
        height: 100%;
        /* Full height */
        background-color: rgba(0, 0, 0, 0.3);
        /* Dark overlay for hover effect */
        transition: width 0.3s ease;
        /* Transition for expanding width */
        z-index: -1;
        /* Position behind the text */
    }

    /* Expand background on hover */
    .rightbtn:hover:after {
        width: 100%;
        /* Expand the overlay color */
    }


    /* Change icon color on hover */
    .rightbtn:hover i {
        color: #000000;
        /* Change icon color to white */
    }


    .rightbtn i {
        font-size: 1.6rem;
    }

    .logosec {
        height: 100%;
        width: 15vw;
        display: flex;
        align-items: center;
        gap: 1vw;
        margin-right: -30%;
        /* Adjusted margin for better alignment */
    }

    .logosec img {
        height: 6vh;
    }

    /* Responsive styles */
    @media (max-width: 910px) {
        .nav {
            height: auto;
            /* Adjust height for content flexibility */
            padding-bottom: 1%;
            /* Adjusted for vertical spacing */
            padding-right: 2%;
        }

        .logo {
            margin-left: 0;
            /* Reset unnecessary margins for mobile */
        }

        .toggle {
            display: block;
            font-size: 2.4rem;
            /* Adjusted for better scaling */
            margin-left: -10%;
        }

        .toggle span {
            font-size: 2.4rem;

        }

        .options {
            position: absolute;
            top: 10vh;
            /* Adjusted top for better visibility */
            left: 0;
            display: none;
            flex-direction: column;
            width: 100%;
            gap: 1.5rem;
            /* Was 15px, now 1.5rem */
            background-color: #f0f0f0;
            padding: 1rem 0;
            /* Was 10px, now 1rem */
            transform: translateY(-100%);
            transition: transform 0.3s ease;
        }

        .options.show {
            display: flex;
            transform: translateY(0);
        }

        .options a {
            padding: 1rem 0;
            /* Was 10px, now 1rem */
            width: 100%;
            text-align: center;
        }

        .rightbtn {
            display: none;
        }

        .logosec {
            display: none;
        }
    }

    /* Smaller screens */
