<script>
    // Register the ScrollTrigger plugin
    gsap.registerPlugin(ScrollTrigger);

    // Select all progress items
    const progressItems = document.querySelectorAll(".progress-item");

    // Create ScrollTrigger for page4
    ScrollTrigger.create({
        trigger: ".page4", // The section that triggers the animation
        start: "top 40%", // Start the animation when the top of page4 is at 40% of the viewport
        end: "bottom top", // End the animation when the bottom of page4 hits the top of the viewport
        onEnter: () => {
            progressItems.forEach((item) => {
                const progressBar = item.querySelector(".progress-bar");
                const progressNumber = item.querySelector(".progress-number");

                // Animate the progress bar width
                gsap.to(progressBar, {
                    width: "90%", // Fill the bar to 90%
                    duration: 1, // Duration of the animation
                    ease: "power1.out" // Easing function for the animation
                });

                // Animate the number from 0 to 90%
                let count = 0; // Initialize the count
                const interval = setInterval(() => {
                    if (count < 90) {
                        count++;
                        progressNumber.innerText = count + "%"; // Update the displayed number
                    } else {
                        clearInterval(interval); // Clear the interval once it reaches 90%
                    }
                }, 10); // Adjust speed of the counting
            });
        },

    });
</script>

